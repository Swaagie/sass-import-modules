{
  "name": "sass-import-modules",
  "version": "3.0.0",
  "description": "SASS/SCSS Import function to import files from node_modules",
  "main": "./dist/index.js",
  "scripts": {
    "prepublish": "mkdir -p dist && babel src --presets es2015 -d dist",
    "test": "mocha --spec reporter --require babel-register ./test/test.js",
    "watch": "mocha --watch --spec reporter --require babel-register ./test/test.js",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- ./test/test.js --require babel-register",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- ./test/test.js --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Swaagie/sass-import-modules.git"
  },
  "bugs": {
    "url": "https://github.com/Swaagie/sass-import-modules/issues"
  },
  "author": "Martijn Swaagman",
  "license": "MIT",
  "devDependencies": {
    "assume": "^2.0.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "istanbul": "^0.4.1",
    "mocha": "^5.0.0",
    "node-sass": "^4.8.0",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "diagnostics": "^2.0.0",
    "resolve": "^1.6.0"
  }
}
